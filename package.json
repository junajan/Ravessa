{
  "name": "sandy",
  "version": "1.0.0",
  "description": "Stock market automated trading system",
  "main": "robot.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "withdraw": "node scripts/transfer.js withdraw",
    "deposit": "node scripts/transfer.js deposit",
    "openings": "node scripts/openings.js",
    "expected-income": "node scripts/expectedIncome.js",
    "history": "node scripts/history.js",
    "history-barchart": "node scripts/historyBarchart.js",
    "history-intraday": "node scripts/historyIntraday.js",
    "start-dev": "nodemon -e '.html,.js,.ejs,.css,.json' app.js",
